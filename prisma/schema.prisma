generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Expense {
  id        String   @id @default(cuid())
  title     String
  amount    Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
